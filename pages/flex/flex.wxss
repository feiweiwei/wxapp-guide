.box {
  background-color: white;
  margin: 0 0 55rpx;
  display: flex;
}

.box-tall {
  height: 800rpx;
}

.box-item {
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  vertical-align: middle;
  margin: 5rpx;
  background-color: #ff6a06;
  font-size: 80rpx;
  color: white;
  text-align: center;
}

.item-tall {
  height: 400rpx;
  line-height: 400rpx;
}

.box-1 {
  flex-direction: row;
}

.box-2 {
  flex-direction: row;
  flex-wrap: nowrap;
}

.box-3 {
  flex-direction: row;
  flex-wrap: wrap;
}

.box-4 {
  flex-direction: row;
  flex-wrap: wrap-reverse;
}

.box-5 {
  flex-flow: row nowrap;
}

.box-6 {
  justify-content: flex-start;
}

.box-7 {
  justify-content: flex-end;
}

.box-8 {
  justify-content: center;
}

.box-9 {
  justify-content: space-between;
}

.box-10 {
  justify-content: space-around;
}

.box-11 {
  align-items: flex-start;
}

.box-12 {
  align-items: flex-end;
}

.box-13 {
  align-items: center;
}

.box-14 {
  align-items: baseline;
}

.box-14 .box-item {
  font-size: 88rpx;
  line-height: initial;
  text-decoration: underline;
}

.box-14 .item-tall {
  font-size: 122rpx;
  line-height: initial;
}

.box-15 {
  align-items: stretch;
}

.box-15 .box-item {
  height: auto;
}

.box-16 {
  flex-wrap: wrap;
  align-content: flex-start;
}

.box-17 {
  flex-wrap: wrap;
  align-content: flex-end;
}

.box-18 {
  flex-wrap: wrap;
  align-content: center;
}

.box-19 {
  flex-wrap: wrap;
  align-content: space-between;
}

.box-20 {
  flex-wrap: wrap;
  align-content: space-around;
}

.box-21 {
  flex-wrap: wrap;
  align-content: stretch;
}

.box-21 .box-item {
  height: auto;
}

.box-22 .order {
  order: -1;
}

.box-item div {
  font-size: 14rpx;
  position: relative;
  top: -150rpx;
}

.box-23 .box-item {
  flex-grow: 1;
  width: auto;
}

.box-23	.grow-2 {
  flex-grow: 2;
}

.box-24 .box-item {
  width: 400rpx;
}

.box-24 .shrink {
  flex-shrink: 0;
}

.box-25 .box-item {
  flex-basis: 200rpx;
  width: 400rpx;
}

.box-26 {
  height: 400rpx;
}

.box-26 .box-item {
  align-self: flex-start;
}

.box-26 .end {
  align-self: flex-end;
}
